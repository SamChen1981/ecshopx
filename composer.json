{
    "name": "shopub/shopub",
    "description": "the new shop",
    "type": "project",
    "keywords": [
        "shopub",
        "php"
    ],
    "homepage": "http://shopub.cn/",
    "license": "Apache-2.0",
    "support": {
        "issues": "https://github.com/shopub/shopub/issues?state=open",
        "forum": "https://www.shopub.cn/forum/",
        "wiki": "https://www.shopub.cn/wiki/",
        "source": "https://github.com/shopub/shopub"
    },
    "require": {
        "php": "^7.1.3",
        "topthink/framework": "5.1.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "think-path": "vendor/topthink/framework"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@php process.php"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
